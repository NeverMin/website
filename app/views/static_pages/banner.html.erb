<h1>banner</h1>


<div class="row">
  <div class="col-xs-12 col-sm-12 col-md-12">
    <!-- Carousel
    ================================================== -->
    <div id="carouselFade" class="carousel slide carousel-fade home-banner-black" data-ride="carousel">
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <%= image_tag("tianhua-home-banner-squashed.jpg", alt: "First slide", class: "") %>
          <div class="container">
            <div class="carousel-caption">
              <h1 class="tianhua-font"><%= t("home.banner") %></h1>
              <h1>WATERFRONT CITY</h1>
            </div>
          </div>
        </div>
        <div class="item">
          <%= image_tag("tianhua-home-banner-squashed.jpg", alt: "Second slide", class: "") %>
          <div class="container">
            <div class="carousel-caption">
              <h1 class="tianhua-font">重庆龙湖昱湖壹号城市展厅</h1>
              <h1>WATERFRONT CITY</h1>
            </div>
          </div>
        </div>
        <div class="item">
          <%= image_tag("tianhua-home-banner-squashed.jpg", alt: "Third slide", class: "") %>
          <div class="container">
            <div class="carousel-caption">
              <h1 class="tianhua-font">重庆龙湖昱湖壹号城市展厅</h1>
              <h1>WATERFRONT CITY</h1>
            </div>
          </div>
        </div>
      </div>
      <a class="left carousel-control" href="#carouselFade" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#carouselFade" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
</div>
